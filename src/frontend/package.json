    "electron-log": "^2.2.17",
    "element-ui": "^2.4.11",
    "lodash": "^4.17.11",
    "salp-course-example": "^1.0.0",
    "vue": "^2.5.21",
    "vue-i18n": "^8.5.0",
    "vue-router": "^3.0.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@ascendancyy/vue-cli-plugin-stylelint": "^1.1.0",
